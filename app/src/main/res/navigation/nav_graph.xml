<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/nav_graph"
        app:startDestination="@id/Missions_fragment">

    <fragment
            android:id="@+id/Missions_fragment"
            android:name="com.upstart13.legba.fragment.MissionsListFragment" >
        <action
                android:id="@+id/action_Missions_fragment_to_missionFragment"
                app:destination="@id/missionFragment" />
    </fragment>
    <fragment
            android:id="@+id/missionFragment"
            android:name="com.upstart13.legba.fragment.MissionFragment"
            android:label="fragment_mission"
            tools:layout="@layout/fragment_mission" >
        <argument
                android:name="mission"
                app:argType="com.upstart13.legba.data.dto.Mission" />
    </fragment>
</navigation>