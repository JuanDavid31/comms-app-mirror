<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/principal_layout_item"
    android:layout_width="match_parent"
    android:layout_height="71.5dp"
    android:layout_marginTop="6.5dp"
    android:layout_marginHorizontal="3dp"
    android:background="@drawable/channel_group_check_item_shape">

    <ImageView
        android:id="@+id/channel_photo"
        android:layout_width="38dp"
        android:layout_height="37.5dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="16.8dp"
        android:layout_marginRight="251.8dp"
        android:layout_marginBottom="17.3dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:srcCompat="@sample/channel" />

    <TextView
        android:id="@+id/channel_name_text"
        style="@style/builtTitlingRgBoldFont"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="67.8dp"
        android:layout_marginTop="22.8dp"
        android:layout_marginRight="167.8dp"
        android:layout_marginBottom="36.3dp"

        android:textSize="16.1sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:text="ALPHA TEAM" />

    <TextView
        android:id="@+id/channel_type_text"
        style="@style/openSansRegularFont"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="67.8dp"
        android:layout_marginTop="41.3dp"
        android:layout_marginRight="164dp"
        android:layout_marginBottom="20dp"

        android:letterSpacing="0.02"
        android:textColor="@color/paleRed"
        android:textSize="10.1sp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Primary Channel" />

    <ImageView
        android:id="@+id/channel_check_img"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:layout_marginLeft="275dp"
        android:layout_marginTop="29.3dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="30.3dp"
        android:src="@mipmap/group_158"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/group_158" />

</androidx.constraintlayout.widget.ConstraintLayout>